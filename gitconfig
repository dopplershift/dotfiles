[user]
	name = Ryan May
	email = rmay31@gmail.com
	signingkey = 33BF97C62E8ACC62
[alias]
	authors = ! git log --format='%aN' | sort -u
	new = !sh -c 'git log --topo-order --reverse $1@{1}..$1@{0} "$@"'
	st = status -sb
	ci = commit
	ds = diff --staged
	co = checkout
	br = branch
	last = log -1 HEAD
	unstage = reset HEAD --
	clear = checkout --
	ribbon = tag --force _ribbon origin/edge5
	catchup = log --stat --reverse --topo-order _ribbon..origin/edge5
	ignore = update-index --assume-unchanged
	g = grep --break --heading --line-number
	pl = log --graph --full-history --all --color --pretty=format:"%x1b[31m%h%x09%x1b[32m%d%x1b[0m%x20%s"
[color]
	ui = auto
[diff]
	tool = meld
[merge]
	tool = meld
[push]
	default = upstream
[grep]
	extendRegexp = true
	lineNumber = true
[core]
	editor = vim
	pager = diff-highlight | less -r
[commit]
	gpgsign = true
[gpg]
	program = gpg2
[merge "jupyternotebook"]
	driver = git-nbmergedriver merge %O %A %B %L %P
	name = jupyter notebook merge driver
[diff "jupyternotebook"]
	command = git-nbdiffdriver diff
